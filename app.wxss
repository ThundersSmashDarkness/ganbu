page,view,text,swiper,swiper-item,image,navigator{
  padding: 0rpx;
  margin: 0rpx;
  box-sizing: border-box;
}


page{
  --themeColor1: #0ac496;
  --themeColor2: #3f7ffc;
 
}
image{
  width: 100%;
}
.van-swipe-cell__left{
  width: 130rpx;
  height: 100%;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  background-color: #f44;
  word-wrap: break-word;
  padding: 0 22rpx;
  box-sizing: border-box;
  /*文本垂直居中*/
  display: flex;
  align-items: center;
  justify-content: center;
}
 
.van-swipe-cell__right {
  width: 130rpx;
  height: 100%;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  background-color: #f44;
  word-wrap: break-word;
  padding: 0 22rpx;
  box-sizing: border-box;
  /*文本垂直居中*/
  display: flex;
  align-items: center;
  justify-content: center;
}